<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="actor" class="com.stakeroute.domain.Actor">
        <constructor-arg name="name" value="Bhawana Mital"/>
        <constructor-arg name="gender" value="Female"/>
        <constructor-arg name="age" value="22"/>
    </bean>

    <bean id="actor1" class="com.stakeroute.domain.Actor">
        <constructor-arg name="name" value="Shibani Mital"/>
        <constructor-arg name="gender" value="Female"/>
        <constructor-arg name="age" value="20"/>
    </bean>

    <bean id="actor2" class="com.stakeroute.domain.Actor">
        <constructor-arg name="name" value="Tushar Mital"/>
        <constructor-arg name="gender" value="Male"/>
        <constructor-arg name="age" value="19"/>
    </bean>

    <bean name="MovieA, MovieB" class="com.stakeroute.domain.Movie" scope="prototype">
        <constructor-arg name="actor" ref="actor" />
    </bean>






</beans>